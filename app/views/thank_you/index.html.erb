<main role="main">

<h3>Welcome...</h3>
<% if user_signed_in? %>
  <h3>You've signed up...</h3>
  
  <%= link_to 'Products', products_path, class: 'btn btn-success btn-large' %>
<% else %>
  <h3>Sign up to view and place orders.</h3>
  <%= link_to 'Sign up', new_user_registration_path, class: 'btn btn-primary btn-large' %>
<% end %>
</main>